{
    PyObject* m = NULL;

    m = PyModule_Create(&amp;globex_module);

    if (m == NULL) {
        goto except;
    }
    /* Adding module globals */
    if (PyModule_AddIntConstant(m, NAME_INT, 42)) {
        goto except;
